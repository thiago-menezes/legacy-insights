{
  "family": "td-ser-lps-front-staging",
  "containerDefinitions": [
    {
      "name": "ser-lps-front-staging",
      "cpu": 0,
      "essential": true,
      "image": "992382784330.dkr.ecr.us-west-2.amazonaws.com/ser-lps-front-staging:v1.0.0-staging.1",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ser-lps-front-staging",
          "awslogs-create-group": "true",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 3000,
          "hostPort": 3000,
          "name": "port",
          "protocol": "tcp"
        }
      ],
      "readonlyRootFilesystem": false,
      "secrets": [
        {
          "name": "STRAPI_URL",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:STRAPI_URL::"
        },
        {
          "name": "STRAPI_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:STRAPI_TOKEN::"
        },
        {
          "name": "NEXT_PUBLIC_INSTITUTION",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:NEXT_PUBLIC_INSTITUTION::"
        },
        {
          "name": "API_BASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:API_BASE_URL::"
        },
        {
          "name": "API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:API_KEY::"
        },
        {
          "name": "CHECKOUT_API_BASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:CHECKOUT_API_BASE_URL::"
        },
        {
          "name": "CHECKOUT_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:CHECKOUT_API_KEY::"
        },
        {
          "name": "CDN_URL",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:CDN_URL::"
        },
        {
          "name": "HUBSPOT_PORTAL_ID",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:HUBSPOT_PORTAL_ID::"
        },
        {
          "name": "HUBSPOT_FORM_ID",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:HUBSPOT_FORM_ID::"
        },
        {
          "name": "HUBSPOT_MARCA",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:HUBSPOT_MARCA::"
        },
        {
          "name": "HUBSPOT_ORIGEM_LEAD",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:992382784330:secret:staging/ser-lps-front-tDkYV2:HUBSPOT_ORIGEM_LEAD::"
        }
      ]
    }
  ],
  "cpu": "1024",
  "enableFaultInjection": false,
  "executionRoleArn": "arn:aws:iam::992382784330:role/GDEV_007_ECSTaskExecutionRoleForStaging",
  "memory": "2048",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "taskRoleArn": "arn:aws:iam::992382784330:role/GDEV_003_ECSTaskRoleForStaging"
}
